<p id="notice"><%= notice %></p>

<h1>Listing Favarites</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Report</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @favarites.each do |favarite| %>
      <tr>
        <td><%= favarite.user_id %></td>
        <td><%= favarite.report_id %></td>
        <td><%= link_to 'Show', favarite %></td>
        <td><%= link_to 'Edit', edit_favarite_path(favarite) %></td>
        <td><%= link_to 'Destroy', favarite, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Favarite', new_favarite_path %>
